  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch AirFry",
      "program": "${workspaceFolder}/src/cli.ts",
      "request": "launch",
      "preLaunchTask": "build",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch CliFry",
      "program": "${workspaceFolder}/src/clifry.ts",
      "request": "launch",
      "preLaunchTask": "build",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "args": ["-t", "simple"]
    }
  ]
}
