{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Templer",
      "program": "${workspaceFolder}/src/cli.ts",
      "request": "launch",
      "preLaunchTask": "build",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Launch CliFry",
      "program": "${workspaceFolder}/node_modules/@danglingdev/clifry/lib/clifry.js",
      "request": "launch",
      "preLaunchTask": "build",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "args": ["-t", "advanced", "-n", "${workspaceFolder}/lib/cli.js"]
    }
  ]
}
