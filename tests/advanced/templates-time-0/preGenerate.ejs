---
generate: /preGenerate/*
---

<script generate>
  (() => {
    const now = "2021-nov-15";
    const version = 1.4;

    const toLowerCase = function(text) {
      return text.toLowerCase() + " hello";
    };
    return {
      siteFiles: {
        "/version.json": {
          createdDate: now,
          version: version,
        },
      },
      global: {
        date: now,
        toLowerCase: toLowerCase,
      },
    };
  })
</script>